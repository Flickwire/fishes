{
  "npm.packageManager": "yarn",
  "editor.codeActionsOnSave": {
    "source.fixAll": true
  },
  "search.exclude": {
    "**/.yarn": true,
    "**/.pnp.*": true
  },
  "typescript.tsdk": ".yarn/sdks/typescript/lib",
  "typescript.enablePromptUseWorkspaceTsdk": true,
  "prettier.prettierPath": ".vscode/pnpify/prettier/index.js",
}
